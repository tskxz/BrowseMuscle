{{#if alert}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
    {{alert}}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/if}}
{{#each rows}}
<tr>
    <td>{{this.id}}</td>
    <td>{{this.alimento}}</td>
    <td>{{this.proteina}}</td>
    <td>{{this.carbs}}</td>
    <td>{{this.gordura}}</td>
    <td>{{this.calorias}}</td>
    <td>{{this.marca}}</td>
    <td class="text-end">
        <a href="/alimento/{{this.id}}" type="button" class="btn btn-light btn-small"><i
                class="bi bi-pencil-square"></i> Visualizar</a>
    </td>
</tr>
{{/each}}