{{#if alert}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
  {{alert}}
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/if}}
{{#each rows}}
<tr>
  <td>{{this.id}}</td>
  <td><a href="/alimento/{{this.id}}">{{this.alimento}}</a></td>
  <td>{{this.calorias}}kcal P:{{this.proteina}} C:{{this.carbs}} G:{{this.gordura}}</td>
  <td>{{this.marca}}</td>
</tr>
{{/each}}