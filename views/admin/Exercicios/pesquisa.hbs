{{#if alert}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
  {{alert}}
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/if}}
{{#each rows}}
<tr>
  <td>{{this.id}}</td>
  <td>{{this.exercicio}}</td>
  <td>{{this.equipamento}}</td>
  <td>{{this.dificuldade}}</td>
  <td>{{this.musculo}}</td>
  <td class="text-end">
    <a href="/admin/adicionar_video_exercicio/{{this.id}}" type="button" class="btn btn-light btn-small"><svg
        xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-camera-video"
        viewBox="0 0 16 16">
        <path fill-rule="evenodd"
